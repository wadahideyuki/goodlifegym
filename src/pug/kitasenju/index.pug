        article.trainerZone
          .u-enTxt TRAINER
          .u-inner
            h2.u-ttl1 ​トレーナー紹介
          #app.trainerBox.box2
            template(v-for="(item, index) in posts")
              .col(v-if="item.acf.store.includes('北千住店')")
                .ph
                  img(v-bind:src="item.acf.photo1", loading="lazy")
                  img.cov(v-bind:src="item.acf.photo2", loading="lazy")
                h3.taC
                  span {{item.title.rendered}}
                  div(v-html="item.content.rendered")

          //.trainerBox.box2
            .col 
              .ph
                +img("trainer/ph10-1.png?20210622")
                +img("trainer/ph10-2.png?2021062","","cov")
              h3.taC #[span ダイチ]#[br]メイントレーナー#[br]

        article.outlineZone 
          h2.ttl2  お客様の声
          ul.voice 
            li 
              img(src="img/v1.jpg")
            li 
              img(src="img/v2.jpg")
          .voiceBtn
            a(href="https://www.google.com/maps/place/GOODLIFE+GYM+%E5%8C%97%E5%8D%83%E4%BD%8F/@35.7473648,139.8077661,17z/data=!4m8!3m7!1s0x60188f144aafdf7d:0x7a58de1bf6742c8d!8m2!3d35.7473605!4d139.8099548!9m1!1b1!16s%2Fg%2F11q9p_c1dx?hl=ja" target="_blank") もっと見る

        .mapZone
          iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3238.1401248539696!2d139.80995479999999!3d35.7473605!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x60188f144aafdf7d%3A0x7a58de1bf6742c8d!2zR09PRExJRkUgR1lNIOWMl-WNg-S9jw!5e0!3m2!1sja!2sjp!4v1655262220032!5m2!1sja!2sjp' width='600' height='450' style='border:0;' allowfullscreen= loading='lazy' referrerpolicy='no-referrer-when-downgrade')


        article.priceZone#price
          .u-enTxt PRICE
          .u-inner
            h2.u-ttl1 ​料金のご案内
            p スケジュールに合わせて通い放題・週1プランからご選択いただけます。
            .planArea
              .planBox
                .contWrap.is-active
                  ul.planList
                    li
                      .lm_ic 人数限定
                      h3 セミパ通い放題
                      span.monthly 通常料⾦：24,800円#[br.sp]→期間限定：#[em 19,800]円(税込)
                      p.recommend セミパーソナルトレーニング#[br.sp]通い放題#[br]【他店舗相互利用可能】
                      
                    li
                      h3 セミパ週１回
                      span.monthly 月額#[em 14,800]円(税込)
                      p.recommend セミパーソナルトレーニング#[br]週１回（月４～５回分）​​
                    li
                      .lm_ic 人数限定
                      h3 マンツーマン通い放題
                      span.monthly 通常料⾦：44,800円#[br.sp]→期間限定：#[em 34,800]円(税込)
                      p.recommend マンツーマンパーソナル#[br.sp]トレーニング通い放題#[br]【他店舗相互利用可能】
                    li
                      h3 マンツーマン週１回
                      span.monthly 月額#[em 24,800]円(税込)
                      p.recommend マンツーマン#[br.sp]パーソナルトレーニング#[br]週１回（月４～５回分）
                    li
                      .lm_ic 人数限定
                      h3 マンツーマン/セミパ#[br.sp]どっちも通い放題
                      span.monthly 通常料⾦：59,800円#[br.sp]→期間限定：#[em 49,800]円(税込)
                      p.recommend マンツーマンパーソナル#[br.sp]セミパーソナル#[br.sp]通い放題#[br]【他店舗相互利用可能】
                  span.planNote ＊ご入会日より1ヶ月以内でプラン開始日をお選びいただけます#[br]＊月の途中からのプラン開始も可能です#[br](例)4/20ご入会の場合、4/21〜5/20の期間内でプラン開始日をお選びいただけます
                  
                .tableBox
                  table
                    tr
                      th カウンセリング＆初回体験料
                      td 3,000円
                    tr
                      th 入会金・登録料
                      td #[span.price 30,000円] → #[em 【キャンペーン中】初回体験当日のご入会で入会金10,000円]
                    tr
                      th 毎月のお支払い方法
                      td 銀行振込またはクレジットカード#[small (VISA/master/AMEX/JCB/Diners/Discover)]


        article.contactZone
          .u-enTxt#contact CONTACT
          .u-inner
            +ttl1("北千住店 お問い合わせ")
            p.u-lead1 カウンセリング、初回体験をご希望の方は、#[br]以下のメールフォームよりお問合せください。
            // class, action, methodを変更しないでください
            .formBox
